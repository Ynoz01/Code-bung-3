import java.util.Scanner;

class prog {

    public static void main(String[]args)
	{
		Scanner sc = new Scanner(System.in);
		double x = sc.nextDouble();
		
		if(x==0)
			System.out.println("x: " + x);
		else
		{
			if(x>2005)
				x = Math.sqrt(x);
			else
				x = x * (x + x);
				x = x * x;
				System.out.println("x: " + x);
		}
	}
}
